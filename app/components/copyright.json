      }
    },
    "logo": {
      "image": {
        "list": [
          {
            "data-srcset": "images/intro/logo-m.webp",
            "media": "(max-width:1023px)",
            "type": "image/webp"
          },
          {
            "data-srcset": "images/intro/logo-m.png",
            "media": "(max-width:1023px)"
          },
          {
            "data-srcset": "images/intro/logo.webp",
            "type": "image/webp"

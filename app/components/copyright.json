  "intro": {
    "attr": {
      "id": "intro"
    },
    "back": {
      "humans": {
        "list": [
          {
            "data-srcset": "images/intro/humans-m.webp 1x, images/intro/humans-m@2x.webp 2x",
            "media": "(max-width:1023px)",
            "type": "image/webp"
          },
          {
            "data-srcset": "images/intro/humans-m.png 1x, images/intro/humans-m@2x.png 2x",
